# Zenmux (https://zenmux.ai/)
# Log in and copy sessionId and sessionId.sig from browser cookies
ENABLED_PROVIDER=zenmux
ZENMUX_SESSION_ID=your_session_id
ZENMUX_SESSION_SIG=your_session_id_sig

# QWeather (https://dev.qweather.com/)
# Create credentials at console.qweather.com → Project Management
# QWEATHER_LOCATION: 坐标 (eg. 116.41,39.92) | 行政区域代码 | LocationID (https://github.com/qwd/LocationList/blob/master/China-City-List-latest.csv)
QWEATHER_ENABLED=true
QWEATHER_HOST=https://your_host.re.qweatherapi.com
QWEATHER_LOCATION=101010100
QWEATHER_KEY=your_api_key
